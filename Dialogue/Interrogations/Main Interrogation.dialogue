import "res://Dialogue/Interrogations/General Responses.dialogue" as snippets

~ start
if isAnomaly
	if selectedCorrectObject
		if isMissing
			=> snippets/missing
		elif isRotated
			=> snippets/flipped
		elif isDiscoloured
			=> snippets/discolouration
		elif isDuplicated
			=> snippets/duplicated	
		else
			You dumb, something broke
	else
		Why is your {{objectName}} like that?
		{{npcName}}: What do you mean, it's perfectly fine?
else
	{{npcName}}: The fuck are you on about mate?
=> END
